    "react-redux": "^5.0.5",
    "react-router-dom": "^4.2.2",
    "react-scroll": "^1.6.7",
    "react-tap-event-plugin": "^2.0.1",
    "react-translate-component": "^0.14.0",
    "recharts": "^1.0.1",
